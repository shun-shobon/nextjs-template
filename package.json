{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s -c 'lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check './*.config.js' 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:tsc": "tsc",
    "lint-fix": "run-s -c 'fix:*'",
    "lint-fix:eslint": "pnpm lint:eslint --fix",
    "lint-fix:prettier": "prettier --write './*.config.js' 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
