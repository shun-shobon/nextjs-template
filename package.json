{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p -c 'lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check './*.config.js' 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:tsc": "tsc",
    "lint-fix": "run-s -c 'lint-fix:*'",
    "lint-fix:eslint": "pnpm lint:eslint --fix",
    "lint-fix:prettier": "prettier --write './*.config.js' 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "next": "^14.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@shun-shobon/eslint-config-base": "^1.0.10",
    "@shun-shobon/eslint-config-next": "^1.0.6",
    "@shun-shobon/eslint-config-react": "^1.0.3",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/next-plugin": "^2.3.1",
    "eslint": "^8.52.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
