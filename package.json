{
	"name": "nextjs-template",
	"version": "0.1.0",
	"type": "module",
	"description": "Next.js template with vanilla-extract, Storybook, and other tools",
	"license": "MIT",
	"private": true,
	"packageManager": "pnpm@8.15.1",
	"scripts": {
		"build": "next build",
		"build-storybook": "storybook build",
		"dev": "next dev",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"lint:ci": "eslint --max-warnings=0 .",
		"lint:fix": "eslint --fix .",
		"start": "next start",
		"storybook": "storybook dev -p 6006",
